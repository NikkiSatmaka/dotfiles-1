bind 'b tabopen https://www.dustloop.com/forums/index.php?/forum/63-blazblue/
bind 'B tabopen https://lispcookbook.github.io/cl-cookbook/
bind 'c js tri.excmds.tabopen(`https://camelcamelcamel.com/search?sq=${document.location.href}`)
bind 'C tabopen https://forum.colemak.com/
bind 'd tabopen about:addons
bind 'D tabopen about:downloads
bind 'e tabopen https://www.reddit.com/r/emacs/
bind 'f js tri.excmds.tabopen(`https://fakespot.com/analyze?url=${document.location.href}`)
bind 'g tabopen https://www.github.com/
bind 'G tabopen https://www.geekhack.org/
bind 'h tabopen https://news.ycombinator.com/news
