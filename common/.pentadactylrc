" faster wm experementation"{{{
" all window management with "r" and "s"
" straight from vimrc; replace nnoremap for bindings; for commands:
" s/nmap\ \-builtin\ <silent>/command/g
" s/<cr>//g
" ree not re
" see not se
" stt not st
" replace r. and r,

" :silent !bspc is slow; making a command speeds up significantly
" commands "{{{

" "r" is redraw"{{{
" worskpace/Destkop switch"{{{
command ra :silent !bspc desktop -f ^1
command rr :silent !bspc desktop -f ^2
command rs :silent !bspc desktop -f ^3
command rt :silent !bspc desktop -f ^4
command rd :silent !bspc desktop -f ^5
command rh :silent !bspc desktop -f ^6
command rn :silent !bspc desktop -f ^7
command ree :silent !bspc desktop -f ^8
command ri :silent !bspc desktop -f ^9
command ro :silent !bspc desktop -f ^10
"}}}
" move to destkop"{{{
command Ra :silent !bspc window -d ^1
command Rr :silent !bspc window -d ^2
command Rs :silent !bspc window -d ^3
command Rt :silent !bspc window -d ^4
command Rd :silent !bspc window -d ^5
command Rh :silent !bspc window -d ^6
command Rn :silent !bspc window -d ^7
command Re :silent !bspc window -d ^8
command Ri :silent !bspc window -d ^9
command Ro :silent !bspc window -d ^10
"}}}

" moving windows within desktop"{{{
" move to biggest
command rcm :silent !bspc window -s biggest
" directions
command rch :silent !bspc window -s left
command rcn :silent !bspc window -s down
command rce :silent !bspc window -s up
command rci :silent !bspc window -s right
" circulate 
command rperiod :silent !bspc desktop -C forward
command rcomma :silent !bspc desktop -C backward
"}}}

"resize"{{{
command rmh :silent !~/bin/resize.sh left
command rmn :silent !~/bin/resize.sh down
command rme :silent !~/bin/resize.sh up
command rmi :silent !~/bin/resize.sh right
"}}}
" open urxvt
command ru :silent !urxvt &

"}}}

" s becomes select/Show/settings"{{{
"select
command sh :silent !bspc window -f left
command sn :silent !bspc window -f down
command see :silent !bspc window -f up
command si :silent !bspc window -f right
command sl :silent !bspc window -f last

" monocle toggle
command stt :silent !bspc desktop -l next
command ss :silent !bspc window -t sticky
command sf :silent !bspc window -t fullscreen

" gap up and down
command su :silent !bspc config -d focused window_gap $((`bspc config -d focused window_gap` - 4 ))
command sd :silent !bspc config -d focused window_gap $((`bspc config -d focused window_gap` + 4 ))

" preselect"{{{
command sph :silent !bspc window -p left
command spn :silent !bspc window -p down
command spe :silent !bspc window -p up
command spi :silent !bspc window -p right
command spx :silent !bspc window -p cancel
command spd :silent !bspc desktop -c
"}}}
"}}}

"}}}

