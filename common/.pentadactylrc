" graveyard
" " wm experementation"{{{
"
" " "r" is redraw"{{{
" " worskpace/Destkop switch"{{{
" nmap -builtin <silent> ra :silent !bspc desktop -f ^1<cr>
" nmap -builtin <silent> rr :silent !bspc desktop -f ^2<cr>
" nmap -builtin <silent> rs :silent !bspc desktop -f ^3<cr>
" nmap -builtin <silent> rt :silent !bspc desktop -f ^4<cr>
" nmap -builtin <silent> rd :silent !bspc desktop -f ^5<cr>
" nmap -builtin <silent> rh :silent !bspc desktop -f ^6<cr>
" nmap -builtin <silent> rn :silent !bspc desktop -f ^7<cr>
" nmap -builtin <silent> re :silent !bspc desktop -f ^8<cr>
" nmap -builtin <silent> ri :silent !bspc desktop -f ^9<cr>
" nmap -builtin <silent> ro :silent !bspc desktop -f ^10<cr>
" "}}}
" " move to destkop"{{{
" nmap -builtin <silent> Ra :silent !bspc window -d ^1<cr>
" nmap -builtin <silent> Rr :silent !bspc window -d ^2<cr>
" nmap -builtin <silent> Rs :silent !bspc window -d ^3<cr>
" nmap -builtin <silent> Rt :silent !bspc window -d ^4<cr>
" nmap -builtin <silent> Rd :silent !bspc window -d ^5<cr>
" nmap -builtin <silent> Rh :silent !bspc window -d ^6<cr>
" nmap -builtin <silent> Rn :silent !bspc window -d ^7<cr>
" nmap -builtin <silent> Re :silent !bspc window -d ^8<cr>
" nmap -builtin <silent> Ri :silent !bspc window -d ^9<cr>
" nmap -builtin <silent> Ro :silent !bspc window -d ^10<cr>
" "}}}
"
" " moving windows within desktop"{{{
" " move to biggest
" nmap -builtin <silent> rcm :silent !bspc window -s biggest<cr>
" " directions
" nmap -builtin <silent> rch :silent !bspc window -s left<cr>
" nmap -builtin <silent> rcn :silent !bspc window -s down<cr>
" nmap -builtin <silent> rce :silent !bspc window -s up<cr>
" nmap -builtin <silent> rci :silent !bspc window -s right<cr>
" " circulate 
" nmap -builtin <silent> r. :silent !bspc desktop -C forward<cr>
" nmap -builtin <silent> r, :silent !bspc desktop -C backward<cr>
" "}}}
"
" "resize"{{{
" nmap -builtin <silent> rmh :silent !~/bin/resize.sh left<cr>
" nmap -builtin <silent> rmn :silent !~/bin/resize.sh down<cr>
" nmap -builtin <silent> rme :silent !~/bin/resize.sh up<cr>
" nmap -builtin <silent> rmi :silent !~/bin/resize.sh right<cr>
" "}}}
" " open urxvt
" nmap -builtin <silent> ru :silent !urxvt &<cr>
"
" "}}}
"
" " s becomes select/Show/settings"{{{
" "select
" nmap -builtin <silent> sh :silent !bspc window -f left<cr>
" nmap -builtin <silent> sn :silent !bspc window -f down<cr>
" nmap -builtin <silent> se :silent !bspc window -f up<cr>
" nmap -builtin <silent> si :silent !bspc window -f right<cr>
" nmap -builtin <silent> sl :silent !bspc window -f last<cr>
"
" " monocle toggle
" nmap -builtin <silent> st :!bspc desktop -l next<cr>
" nmap -builtin <silent> ss :silent !bspc window -t sticky<cr>
" nmap -builtin <silent> sf :silent !bspc window -t fullscreen<cr>
"
" " gap up and down
" nmap -builtin <silent> su :silent !bspc config -d focused window_gap $((`bspc config -d focused window_gap` - 4 ))<cr>
" nmap -builtin <silent> sd :silent !bspc config -d focused window_gap $((`bspc config -d focused window_gap` + 4 ))<cr>
"
" " preselect"{{{
" nmap -builtin <silent> sph :silent !bspc window -p left<cr>
" nmap -builtin <silent> spn :silent !bspc window -p down<cr>
" nmap -builtin <silent> spe :silent !bspc window -p up<cr>
" nmap -builtin <silent> spi :silent !bspc window -p right<cr>
" nmap -builtin <silent> spx :silent !bspc window -p cancel<cr>
" nmap -builtin <silent> spd :silent !bspc desktop -c<cr>
" "}}}
" "}}}
