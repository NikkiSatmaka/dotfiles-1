! key left of space
! possibly switch cedilla and return
! on release window management/dropdown/scrot/brightness/volume/etc. key and tab
keycode 102 = Hyper_L
add mod4 = Hyper_L

! right of space
! shift and enter
keycode 100 = Shift_L
! altgr and cedilla (c-backspace behaviour)
keycode 101 = ISO_Level3_Shift

! make sure return key exists
keycode 221 = Return
! make sure cedilla exists
keycode 220 = cedilla

! fix a few keys
keycode 97 = slash question backslash
keycode 51 = apostrophe quotedbl

! vim: set ft=xmodmap:
